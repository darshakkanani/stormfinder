# üîë Stormfinder Provider Configuration Template
# Copy this file to ~/.config/stormfinder/provider-config.yaml and add your API keys

# üÜì Free API Keys (Recommended)
github:
  - "YOUR_GITHUB_TOKEN_HERE"        # Get from: https://github.com/settings/tokens
  # Scope: public_repo (for public repositories)

virustotal:
  - "YOUR_VIRUSTOTAL_API_KEY_HERE"  # Get from: https://www.virustotal.com/gui/join-us
  # Free tier: 4 requests/minute

securitytrails:
  - "YOUR_SECURITYTRAILS_API_KEY"   # Get from: https://securitytrails.com/corp/api
  # Free tier: 50 queries/month

# üí∞ Paid API Keys (Enhanced Results)
shodan:
  - "YOUR_SHODAN_API_KEY_HERE"      # Get from: https://account.shodan.io/
  # Paid plans for unlimited queries

chaos:
  - "YOUR_CHAOS_API_KEY_HERE"       # Get from: https://chaos.projectdiscovery.io/
  # ProjectDiscovery's subdomain dataset

censys:
  - "YOUR_CENSYS_API_ID:YOUR_CENSYS_SECRET"  # Get from: https://censys.io/account/api

fullhunt:
  - "YOUR_FULLHUNT_API_KEY"         # Get from: https://fullhunt.io/

# üîç Search Engine APIs
binaryedge:
  - "YOUR_BINARYEDGE_API_KEY"       # Get from: https://app.binaryedge.io/

zoomeye:
  - "YOUR_ZOOMEYE_API_KEY"          # Get from: https://www.zoomeye.org/api

fofa:
  - "YOUR_FOFA_EMAIL:YOUR_FOFA_KEY" # Get from: https://fofa.so/

# üì± Social Media APIs (Optional)
twitter:
  - "YOUR_TWITTER_BEARER_TOKEN"     # Get from: https://developer.twitter.com/

# üåê DNS/Certificate APIs
whoisxmlapi:
  - "YOUR_WHOISXML_API_KEY"         # Get from: https://whoisxmlapi.com/

certspotter:
  - "YOUR_CERTSPOTTER_API_KEY"      # Get from: https://sslmate.com/certspotter/api/

# üìä Threat Intelligence APIs
threatbook:
  - "YOUR_THREATBOOK_API_KEY"       # Get from: https://threatbook.cn/

intelx:
  - "YOUR_INTELX_API_KEY"           # Get from: https://intelx.io/

# üîß Additional APIs
builtwith:
  - "YOUR_BUILTWITH_API_KEY"        # Get from: https://builtwith.com/api

hunter:
  - "YOUR_HUNTER_API_KEY"           # Get from: https://hunter.io/api

# üí° Tips:
# 1. You don't need all API keys - Stormfinder works great with just free sources
# 2. Start with GitHub and VirusTotal for best results
# 3. Add more keys gradually to increase discovery
# 4. Keep this file secure and never commit it to version control
# 5. Use environment variables for CI/CD: STORMFINDER_GITHUB_TOKEN, etc.
